{
  "name": "alexie",
  "version": "0.0.1",
  "main": "index.js",
  "engines": {
    "node": "8.4.0",
    "npm": "5.3.0"
  },
  "scripts": {
    "server": "nodemon src/app.js --exec babel-node --presets env",
    "start": "node ./dist/src/app.js",
    "build": "babel src/*.js -d dist",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Munsterberg",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^3.5.0",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.2",
    "mongoose": "^4.12.5",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.10.0",
    "nodemon": "^1.12.1"
  }
}
